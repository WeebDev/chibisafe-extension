{
	"name": "chibisafe-extension",
	"version": "1.0.0",
	"description": "Uploader for chibisafe.",
	"repository": "https://github.com/WeebDev/chibisafe-extension.git",
	"author": "pilar6195 (https://github.com/pilar6195)",
	"license": "MIT",
	"engines": {
		"node": ">=8.0.0"
	},
	"dependencies": {
		"eslint": "^7.17.0",
		"eslint-config-aqua": "^8.0.0"
	},
	"eslintConfig": {
		"extends": [
			"aqua"
		],
		"env": {
			"browser": true,
			"es6": true,
			"webextensions": true
		},
		"ignorePatterns": [
			"axios.min.js",
			"browser-polyfill.min.js"
		],
		"rules": {
			"func-names": 0,
			"curly": 0,
			"max-len": 0,
			"id-length": 0,
			"no-invalid-this": 0,
			"no-console": 0,
			"no-unused-expressions": [
				"error",
				{
					"allowTernary": true
				}
			],
			"comma-dangle": [
				"error",
				"always-multiline"
			]
		}
	}
}
