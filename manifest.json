	{
		"manifest_version": 2,
		"name": "lolisafe Uploader",
		"description": "Uploader for lolisafe",
		"version": "2019.1.24.1",
		"version_name": "Biribiri",
		"options_ui": {
			"page": "settings.html",
			"chrome_style": false,
			"open_in_tab": true
		},
		"icons": {
			"48": "logo-48x48.png",
			"128": "logo-128x128.png"
		},
		"permissions": [
			"contextMenus",
			"storage",
			"notifications",
			"tabs",
			"webRequest",
			"webRequestBlocking",
			"<all_urls>"
		],
		"background": {
			"scripts": [
				"axios.min.js",
				"background.js"
			],
			"persistent": true
		},
		"web_accessible_resources": [
			"content.js",
			"content.css"
		]
	}
